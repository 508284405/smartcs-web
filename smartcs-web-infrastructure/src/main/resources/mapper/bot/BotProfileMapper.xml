<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leyue.smartcs.bot.mapper.BotProfileMapper">

    <!-- 根据prompt模板key查询使用该模板的机器人数量 -->
    <select id="countByPromptKey" resultType="int">
        SELECT COUNT(*)
        FROM t_cs_bot_profile
        WHERE prompt_key = #{promptKey}
        AND is_deleted = 0
    </select>

</mapper> 