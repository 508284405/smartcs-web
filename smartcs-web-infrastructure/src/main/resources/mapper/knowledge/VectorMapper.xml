<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.leyue.smartcs.knowledge.mapper.VectorMapper">

    <!-- 结果映射 -->
    <resultMap id="BaseResultMap" type="com.leyue.smartcs.knowledge.dataobject.VectorDO">
        <id column="id" property="id" jdbcType="BIGINT"/>
        <result column="chunk_id" property="chunkId" jdbcType="BIGINT"/>
        <result column="vector_data" property="vectorData" jdbcType="LONGVARCHAR"/>
        <result column="vector_dim" property="vectorDim" jdbcType="INTEGER"/>
        <result column="provider" property="provider" jdbcType="VARCHAR"/>
        <result column="model_name" property="modelName" jdbcType="VARCHAR"/>
        <result column="created_by" property="createdBy" jdbcType="BIGINT"/>
        <result column="created_at" property="createdAt" jdbcType="BIGINT"/>
        <result column="updated_at" property="updatedAt" jdbcType="BIGINT"/>
    </resultMap>

    <!-- 基础字段 -->
    <sql id="Base_Column_List">
        id, chunk_id, vector_data, vector_dim, provider, model_name, created_by, created_at, updated_at
    </sql>

</mapper> 